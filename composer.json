{
    "name": "telegraph/telegraph",
    "type": "library",
    "description": "A lambda-style PSR-7 middleware dispatcher.",
    "keywords": [
        "lambda",
        "middleware",
        "psr-7"
    ],
    "homepage": "https://github.com/telegraphphp/telegraph.telegraph",
    "license": "MIT",
    "authors": [
        {
            "name": "All telegraph/telegraph contributors",
            "homepage": "https://github.com/telegraphphp/telegraph.telegraph/contributors"
        }
    ],
    "require": {
        "php": ">=7",
        "psr/http-message": "~1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Telegraph\\": "src/"
        }
    },
    "require-dev": {
        "zendframework/zend-diactoros": "~1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Telegraph\\": "tests/"
        }
    }
}
